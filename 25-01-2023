class Employee_1:  
    def __init__(self, name, age,ID):  
        self.name = name  
        self.age = age  
        self.ID = ID  
Emp = Employee_1("dk", 20, 'AAINrs')  
print(Emp.name)  
print(Emp.age)  
print(Emp.ID)  
